{
  "name": "synchive-js-sdk",
  "version": "0.1.0",
  "description": "SyncHive JS SDK",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "oidc-client-ts": "^2.4.0"
  },
  "devDependencies": {
    "tsdown": "^0.7.0",
    "typescript": "^5.4.0"
  }
}
